    "jest": "^29.7.0",
    "jsdom": "^22.1.0",
    "mobx-utils": "^5.6.1",
    "node-forge": "^1.3.1",
    "remark-gfm": "^4.0.0",
    "resolve-url-loader": "^5.0.0",
    "run-script-os": "^1.1.6",
    "sass": "^1.62.1",
    "sass-loader": "^10.4.1",
    "storybook": "^8.0.4",
    "storybook-dark-mode": "^4.0.1",
    "style-loader": "^3.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.1",
