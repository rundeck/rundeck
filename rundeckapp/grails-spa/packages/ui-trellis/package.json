    "mobx": "^5.15.4",
    "mobx-utils": "^5.6.1",
    "mobx-vue": "^2.0.11",
    "moment": "^2.29.4",
    "prex": "^0.4.7",
    "uiv": "^0.33.0",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.3.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "overrides": {
    "node-forge": "^1.3.1"
  }
}
