    "uuid": "^8.3.2",
    "walk": "^2.3.15"
  },
  "peerDependencies": {
    "@rundeck/client": "^0.2.5",
    "ace-builds": "^1.23.4",
    "mobx": "^5.15.4",
    "mobx-utils": "^5.6.1",
    "moment": "^2.29.4",
    "prex": "^0.4.7",
    "vue": "^3.3.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "overrides": {
    "node-forge": "^1.3.1",
    "acorn": "7.3.0"
  }
}
