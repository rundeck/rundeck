{
  "name": "@rundeck/ui-trellis",
  "version": "3.3.7",
  "description": "A collection of utilities used across various javascript applications and components.",
  "main": "./lib/index.js",
  "scripts": {
    "test": "jest --no-cache",
    "test:unit": "jest --silent",
    "dev:test:watch": "npm install && jest --silent --watch",
    "dev:test:unit": "npm install && npm run test:unit",
    "ci:test:unit": "npm ci && npm run test:unit",
    "dev:app:build": "npm install && npm run build:app",
    "ci:app:build": "npm ci && npm run build:app",
    "dev:library:build": "npm install && npm run build:library --prefer-offline --no-audit",
    "ci:library:build": "npm ci && npm run build:library --prefer-offline --no-audit",
    "dev": "VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.config.app.js ./node_modules/.bin/vue-cli-service build --mode development --watch && tsc -p ./tsconfig.app.json && VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.config.library.js ./node_modules/.bin/vue-cli-service build --mode development --watch",
    "build:app": "run-script-os",
    "build:app:darwin:linux": "VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.config.app.js ./node_modules/.bin/vue-cli-service build --mode production && tsc -p ./tsconfig.app.json",
    "build:app:win32": "VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.config.app.js node_modules/.bin/vue-cli-service.cmd build --mode production && tsc -p ./tsconfig.app.json",
    "build:library": "run-script-os",
    "build:library:darwin:linux": "VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.config.library.js ./node_modules/.bin/vue-cli-service build --mode production && tsc -p ./tsconfig.build.json && npm run clean",
    "build:library:win32": "VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.config.library.js vue-cli-service.cmd build --mode production && tsc -p ./tsconfig.build.json && npm run clean",
    "storybook": "storybook dev -p 6006 -- --debug-webpack",
    "clean": "cp -a ./lib/src/library/. ./lib && rm -rf ./lib/src && exit 0",
    "lint": "eslint src --fix",
    "build-storybook": "storybook build",
    "create-wrapper": "npx hygen primevue-generator wrapper --componentName",
    "create-css": "npx hygen primevue-generator css --componentName",
    "storybook-docs": "storybook dev --docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rundeck/ui-trellis.git"
  },
  "author": "Jesse Marple <jesse@rundeck.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rundeck/ui-trellis/issues"
  },
  "homepage": "https://github.com/rundeck/ui-trellis#readme",
  "dependencies": {
    "@esfx/canceltoken": "^1.0.0",
    "@iconfu/svg-inject": "^1.2.3",
    "@popperjs/core": "^2.8.4",
    "@rundeck/client": "^0.2.5",
    "ace-builds": "^1.23.4",
    "axios": "^1.6.4",
    "core-js": "^3.6.5",
    "deep-object-diff": "^1.1.9",
    "dompurify": "^3.0.3",
    "fuse.js": "^6.6.2",
    "jest-junit": "^16.0.0",
    "mitt": "^3.0.0",
    "mobx": "^5.15.4",
    "moment": "^2.30.1",
    "npm": "^10.2.2",
    "perfect-scrollbar": "^1.5.0",
    "primevue": "^3.50.0",
    "uiv": "^2.0.6",
    "vue": "^3.4.25",
    "vue-cookies": "^1.8.3",
    "vue-good-table-next": "^0.2.1",
    "vue-i18n": "^9.2.2",
    "vue-multiselect": "^2.1.6",
