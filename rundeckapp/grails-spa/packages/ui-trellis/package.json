    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-vue": "^9.20.1",
    "eslint-plugin-vuejs-accessibility": "^2.2.0",
