{
  "name": "rundeck-vue-ui",
  "version": "1.0.0",
  "description": "Rundeck User Interface",
  "author": "jesse@rundeck.com",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service build --watch --mode development",
    "dev:live": "vue-cli-service serve --mode live",
    "start": "npm run dev",
    "lint": "eslint --ext .js,.vue src",
    "test": "jest",
    "build": "run-script-os",
    "build:darwin:linux": "./node_modules/.bin/vue-cli-service build --mode production",
    "build:win32": "node_modules/.bin/vue-cli-service.cmd build --mode production",
    "dev:build": "npm install && npm run build",
    "ci:build": "npm ci && npm run build",
    "dev:test": "npm install && npm run test",
    "ci:test": "npm ci && npm run test"
  },
  "dependencies": {
    "@iconfu/svg-inject": "1.2.3",
    "@rundeck/client": "0.2.5",
    "ace-builds": "1.4.12",
    "axios": "0.21.3",
    "brace": "0.11.1",
    "core-js": "3.16.1",
    "fuse.js": "3.6.1",
    "lodash": "4.17.21",
    "markdown-it": "10.0.0",
    "markdown-it-vue": "1.1.7",
    "mobx": "5.15.7",
    "mobx-utils": "5.6.2",
    "mobx-vue": "2.0.11",
    "moment": "2.29.3",
    "prex": "0.4.7",
    "sass": "1.49.7",
    "source-map-loader": "1.1.3",
    "uiv": "0.33.0",
    "vue": "2.6.14",
    "vue-class-component": "7.2.3",
    "vue-cookies": "1.7.4",
    "vue-fuse": "2.2.1",
    "vue-i18n": "8.25.0",
    "vue-moment": "4.1.0",
    "vue-property-decorator": "8.4.1",
    "vue-router": "3.5.2",
    "vue-scrollto": "2.20.0",
    "vue2-filters": "0.9.1",
    "vuex": "3.6.2"
  },
  "devDependencies": {
    "@babel/core": "7.15.0",
    "@rundeck/ui-trellis": "file:../ui-trellis",
    "@storybook/addon-knobs": "6.2.9",
    "@types/jest": "25.2.3",
    "@types/lodash": "4.14.172",
    "@vue/cli-plugin-babel": "4.5.17",
    "@vue/cli-plugin-typescript": "4.5.17",
    "@vue/cli-service": "4.5.17",
    "autoprefixer": "7.2.6",
    "babel-loader": "8.1.0",
    "chalk": "2.4.2",
    "file-loader": "1.1.11",
    "friendly-errors-webpack-plugin": "1.7.0",
    "html-webpack-plugin": "3.2.0",
    "jest": "26.6.3",
    "lerna": "3.22.1",
    "less-loader": "6.2.0",
    "node-notifier": "5.4.5",
    "ora": "1.4.0",
    "pnp-webpack-plugin": "1.7.0",
    "portfinder": "1.0.28",
    "rimraf": "2.6.3",
    "run-script-os": "1.1.6",
    "sass-loader": "8.0.2",
    "semver": "5.7.1",
    "shelljs": "0.7.8",
    "ts-jest": "26.5.6",
    "tslib": "2.3.0",
    "typescript": "4.2.3",
    "vue-jest": "3.0.7",
    "vue-template-compiler": "2.6.14",
    "webpack": "4.46.0"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!bootstrap-vue)"
    ],
    "testURL": "http://localhost/",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "overrides": {
    "node-forge": "^1.3.1"
  }
}
