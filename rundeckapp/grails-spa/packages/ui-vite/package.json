{
  "name": "pagerduty-process-automation",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "audit": "run-s audit:*",
    "audit:dev": "pnpm audit --json --dev",
    "audit:prod": "pnpm audit --json --prod",
    "prebuild": "rimraf dist",
    "build:prod:spa": "cross-env NODE_ENV=production vite build",
    "build:prod:ssg": "cross-env NODE_ENV=production vite-ssg build",
    "dev": "vite --open --host --port 4000",
    "lint": "run-s lint:*",
    "lint:types": "vue-tsc --noEmit",
    "lint:css": "stylelint --fix \"**/*.{css,scss,vue}\"",
    "lint:js": "eslint --fix --ext \"**/*.{js,ts,vue}\"",
    "lint:imports": "import-sort --write \"src/**/*{.js,.ts,.vue}\"",
    "lint:md": "markdownlint --config .markdownlint.json --fix \"**/*.md\"",
    "lint:prettier": "prettier --write --loglevel error \"**/*.{js,ts,json,css,scss,vue,html,md}\"",
    "postinstall": "husky install && chmod ug+x .husky/*",
    "preinstall": "npx only-allow pnpm",
    "preview": "vite preview --port 4000",
    "preview:ci": "start-server-and-test preview http://localhost:4000",
    "preview:https": "serve dist",
    "test:components": "cypress open-ct",
    "test:components:ci": "cypress run-ct",
    "test:e2e": "cypress open",
    "test:e2e:headless": "cypress run",
    "test:e2e:ci": "npm run preview:ci 'cypress run'",
    "test:unit": "echo 'vitest not set up just yet'",
    "test:unit:ci": "echo 'vitest not set up just yet'"
  },
  "dependencies": {
    "@vueuse/core": "^8.1.2",
    "@pinia/testing": "^0.0.10",
    "@vueuse/head": "^0.7.5",
    "axios": "^0.26.0",
    "axios-case-converter": "^0.9.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.12",
    "prism-theme-vars": "^0.2.2",
    "vue": "^3.2.25",
    "vue-demi": "^0.12.1",
    "vue-i18n": "^9.1.9",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.18.9",
    "@commitlint/cli": "^16.0.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@cypress/vite-dev-server": "^2.2.0",
    "@cypress/vue": "^3.1.1",
    "@iconify-json/carbon": "^1.0.9",
    "@intlify/vite-plugin-vue-i18n": "^3.0.0",
    "@types/markdown-it-link-attributes": "^3.0.1",
    "@types/node": "^17.0.19",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "^2.0.0",
    "critters": "^0.0.16",
    "cross-env": "^7.0.3",
    "cypress": "^9.5.1",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "https-localhost": "^4.7.0",
    "husky": "^7.0.4",
    "imagemin-lint-staged": "^0.4.0",
    "import-sort": "^6.0.0",
    "import-sort-cli": "^6.0.0",
    "import-sort-config": "^6.0.0",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-eslint": "^6.0.0",
    "lint-staged": "^12.0.3",
    "markdown-it-link-attributes": "^4.0.0",
    "markdown-it-prism": "^2.2.1",
    "markdownlint-cli": "^0.31.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.3",
    "postcss-html": "^1.2.0",
    "prettier": "^2.4.1",
    "prettier-plugin-import-sort": "^0.0.7",
    "rimraf": "^3.0.2",
    "sass": "^1.43.4",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^14.0.1",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "3.0.0",
    "stylelint-config-recommended-vue": "^1.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "typescript": "^4.6.2",
    "unplugin-auto-import": "^0.6.0",
    "unplugin-icons": "^0.14.0",
    "unplugin-vue-components": "^0.18.3",
    "vite": "^2.6.13",
    "vite-plugin-inspect": "^0.4.3",
    "vite-plugin-md": "^0.11.4",
    "vite-plugin-pages": "^0.22.0",
    "vite-plugin-pwa": "^0.11.3",
    "vite-plugin-vue-layouts": "^0.6.0",
    "vite-plugin-windicss": "^1.5.1",
    "vite-ssg": "^0.19.0",
    "vite-ssg-sitemap": "^0.2.0",
    "vue-tsc": "^0.33.3"
  },
  "engines": {
    "node": ">=14.17.0",
    "pnpm": "^6.32.1"
  }
}
