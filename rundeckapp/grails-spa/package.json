    "ts-rundeck": "0.1.5",
    "uiv": "^0.33.0",
    "vue": "2.6.10",
    "vue-cookies": "^1.5.6",
    "vue-fuse": "^2.2.0",
    "vue-i18n": "^8.14.0",
